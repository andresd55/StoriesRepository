{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "HackerNewsApi": {
        "baseUrl": "https://hacker-news.firebaseio.com/v0/",
        "getItemPath": "item",
        "getNewStoriesIdsPath": "newstories"
    },
    "Cache": {
        "key": "newStoriesCache",
        "expirationSeconds": 60
    },
    "AllowedHosts": "*"
}
